# Merkle Trade Integration Configuration
# Get API key from your Merkle Trade developer dashboard
REACT_APP_MERKLE_API_KEY=demo_key_for_testing

# Network Configuration
# testnet | mainnet
REACT_APP_MERKLE_NETWORK=testnet

# Aptos Network Configuration  
# testnet | mainnet
REACT_APP_APTOS_NETWORK=testnet

# Enable production trading (use with caution)
# true | false
REACT_APP_ENABLE_PRODUCTION=false

# Optional: Aptos Node URL (defaults to official if not set)
REACT_APP_APTOS_NODE_URL=https://api.testnet.aptoslabs.com/v1

# Debug mode
REACT_APP_DEBUG=true