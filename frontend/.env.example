# Merkle Trade Integration Configuration
# Copy this file to .env and fill in your actual API keys

# Merkle Trade API Key
# Get this from your Merkle Trade developer dashboard
REACT_APP_MERKLE_API_KEY=your_merkle_api_key_here

# Optional: Merkle Trade Network Configuration
# testnet | mainnet
REACT_APP_MERKLE_NETWORK=testnet

# Optional: Aptos Network Configuration  
# testnet | mainnet
REACT_APP_APTOS_NETWORK=testnet

# Optional: Enable production trading (use with caution)
# true | false
REACT_APP_ENABLE_PRODUCTION=false